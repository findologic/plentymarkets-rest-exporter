{
    "name": "findologic/plentymarkets-rest-exporter-new",
    "description": "FINDOLOGIC exporter for exporting product data via the plentymarkets REST API",
    "type": "project",
    "authors": [
        {
            "name": "Dominik Brader",
            "email": "d.brader@findologic.com",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6.5",
        "vasily-kartashov/log4php": "^4.1",
        "ext-json": "*",
        "findologic/libflexport": "^2.7",
        "nesbot/carbon": "^2.38",
        "symfony/cache": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/yaml": "^5.0",
        "symfony/var-dumper": "^5.1",
        "symfony/console": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/finder": "^5.1"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "FINDOLOGIC\\PlentyMarketsRestExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FINDOLOGIC\\PlentyMarketsRestExporter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=phpcs.xml .",
        "fix": "./vendor/bin/phpcbf --standard=phpcs.xml .",
        "test": "phpunit --configuration phpunit.xml.dist --coverage-text --coverage-clover=coverage.xml",
        "export": "bin/export",
        "clear": "bin/clearDebugFiles"
    },
    "license": "MIT"
}
